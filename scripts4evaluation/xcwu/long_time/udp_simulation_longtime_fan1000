date >>simulation-time1000.txt
./waf --run "xgpon-simulation-speed-fan-udp  --onus=1000 --dspktinterval=0.08 --uspktinterval=0.32 --astoptime=4000 --sstoptime=4001" >udp-1000-0.15.txt
date >>simulation-time1000.txt


