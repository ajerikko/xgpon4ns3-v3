date >>simulation-time-random6.txt
./waf --run "xgpon-simulation-speed-fan-udp  --onus=1001 --dspktinterval=0.08 --uspktinterval=0.32 --astoptime=500 --sstoptime=501" >udp-random-1001-0.15.txt
date >>simulation-time-random6.txt


