date >>simulation-time-random5.txt
./waf --run "xgpon-simulation-speed-fan-udp  --onus=811 --dspktinterval=0.064 --uspktinterval=0.256 --astoptime=500 --sstoptime=501" >udp-random-811-0.1875.txt
date >>simulation-time-random5.txt

